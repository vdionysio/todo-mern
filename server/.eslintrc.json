{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
    },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_" }]
  }
}
